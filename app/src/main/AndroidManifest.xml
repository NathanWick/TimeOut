<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.winbusiness.timeout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TimeOut"
        android:theme="@style/Theme.AppCompat.Light"
        >
        <activity
            android:name=".LoginActivity"
            android:label="TimeOut">
            <intent-filter android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ManagerHubActivity"
            android:label="Manager Hub"/>
        <activity
            android:name=".EmployeeHubActivity"
            android:label="Employee Hub"/>
        <activity
            android:name=".EmployeeEditorActivity"
            android:label="Employee Editor"/>
        <activity
            android:name=".ClockInOutHistoryActivity"
            android:label="Clock In Out History"/>
    </application>

</manifest>
